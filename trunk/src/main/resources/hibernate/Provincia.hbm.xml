<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.turnolibre.business.ubicacion" default-access="field">

	<class name="Provincia" table="provincias">

		<id name="id">
			<generator class="native" />
		</id>

		<property name="nombre" unique-key="PROVINCIA_BUSINESS_KEY" not-null="true" />

	</class>

</hibernate-mapping>
